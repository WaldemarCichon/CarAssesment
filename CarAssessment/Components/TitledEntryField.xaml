<?xml version="1.0" encoding="UTF-8" ?>
<StackLayout
    x:Class="CarAssessment.Components.TitledEntryField"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    HorizontalOptions="FillAndExpand">

    <Label
        x:Name="TitleLabel"
        TextColor="#111"
        WidthRequest="1000" />
    <Entry
        x:Name="TextEntry"
        HorizontalOptions="FillAndExpand"
        PropertyChanged="TextEntry_PropertyChanged" />
</StackLayout>
